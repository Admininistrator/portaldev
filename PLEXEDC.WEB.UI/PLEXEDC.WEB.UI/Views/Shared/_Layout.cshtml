                                        </li>
                                    </ul>
                                </div>
                            </li>
                        </ul>
                        <script>
                            $(document).ready(function () {
                                setTimeout(function () {
                                    $(".child-block1").find("li.has-submenu").each(function () {
                                        var top_padding = $(this).index() * (-100);
                                        $(this).find(".child-block2").css("top", top_padding + "%");
                                    });
                                    $(".child-block2").find("li.has-submenu").each(function () {
                                        var top_padding = $(this).index() * (-100);
                                        $(this).find(".child-block3").css("top", top_padding + "%");
                                    });


                                    $("#main-navigation > ul").on("show.zf.dropdownmenu", function (ev, $el) {
                                        var _$max_height = $(".main-navigation-panels.js-dropdown-active").height();
                                        $el.find("ul").each(function (index) {
                                            $(this).css("height", "auto");
                                            if ($(this).height() > _$max_height) _$max_height = $(this).height();
                                        });
                                        $(".main-navigation-menu").find("ul").height(_$max_height);

                                        if ($el.hasClass("main-navigation-panels")) {
                                            $("#middle_hover_back").addClass("hover-back");
                                        }
                                        if ($el.hasClass("child-block2")) {
                                            $(".main-navigation-panels .child-block1").parent().addClass("back-hover");
                                            $(".main-navigation-panels .panel-content").css("display", "none");
                                        }
                                    });
                                    $("#main-navigation > ul").on("hide.zf.dropdownmenu", function (ev, $el) {
                                        if ($el.hasClass("is-down-arrow") || $el.hasClass("main-navigation-menu")) {
                                            $("#middle_hover_back").removeClass("hover-back");
                                            $(".main-navigation-menu").find("ul").height("");
                                            $(".main-navigation-panels .child-block1").parent().removeClass("back-hover");
                                            $(".main-navigation-panels .panel-content").css("display", "block");
                                        }
                                        if ($el.hasClass("is-right-arrow") && !$el.hasClass("is-submenu-item")) {
                                            $(".main-navigation-panels .child-block1").parent().removeClass("back-hover");
                                            $(".main-navigation-panels .panel-content").css("display", "block");
                                        }
                                    });

                                }, 300)
                            });
                        </script>

                    </nav>
                </div>

            </div>

        </div>

        </div>
    </header>
    <br />
    <div class="container body-content">
        @RenderBody()

    </div>
    <footer></footer>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
