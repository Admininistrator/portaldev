                            </li>
                            <li role="menuitem" class="has-submenu is-dropdown-submenu-parent is-down-arrow" aria-haspopup="true" aria-selected="false" aria-expanded="false" aria-label="Service Information">
                                <a id="top1_TOPMENU1_rep_parent_menu_hl_header_3" href="#">Service Information</a>
                            </li>
                            <li role="menuitem" class="has-submenu is-dropdown-submenu-parent is-down-arrow" aria-haspopup="true" aria-selected="false" aria-expanded="false" aria-label="My Profile">
                                <a id="top1_TOPMENU1_rep_parent_menu_hl_header_4" href="#">My Profile</a>
                            </li>
                            <li role="menuitem" class="has-submenu is-dropdown-submenu-parent is-down-arrow" aria-haspopup="true" aria-selected="false" aria-expanded="false" aria-label="Contact Us">
                                <a id="top1_TOPMENU1_rep_parent_menu_hl_header_4" href="#">Contact Us</a>
                            </li>


                            <li role="menuitem" class="has-submenu is-dropdown-submenu-parent is-down-arrow" aria-haspopup="true" aria-selected="false" aria-expanded="false" aria-label="" data-is-click="true">
                                <div id="top1_TOPMENU1_rep_parent_menu_hl_header_4" class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-th pull-left"></span></a>
                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                        <li role="presentation">
                                            <a role="menuitem" tabindex="-1" href="@Url.Action("Index", "Home")">Home</a>
                                        </li>
                                        <li role="presentation">
                                            <a role="menuitem" tabindex="-1" href="@Url.Action("Account", "Home")">My Account</a>
                                        </li>
                                        <li role="presentation">
                                            <a role="menuitem" tabindex="-1" href="@Url.Action("Trade", "Home")">
                                                Trade
                                            </a>
                                        </li>
                                        @*<li role="presentation" class="divider"></li>*@
                                        <li role="presentation">
                                            <a role="menuitem" tabindex="-1" href="#">Service Information</a>
                                        </li>
                                        <li role="presentation">
                                            <a role="menuitem" tabindex="-1" href="#">My Profile</a>
                                        </li>
                                        <li role="presentation">
