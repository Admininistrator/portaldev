@model PLEXEDC.WEB.UI.Models.ServiceRequestViewModel


<div class="modal fade" role="dialog" id="showMoreModal" aria-labelledby="labeltransactioListing" aria-hidden="true" tabindex="-1" style="color:black; overflow:auto">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="labeltransactioListing">
                    All Service Request
                </h4>
            </div>
            <div class="modal-body" style="overflow:auto;">

                <div class="row">
                    <div class="panel">
                        <div class="panel-body">
                            <div class="row">

                                <table class="table">
                                    <thead>
                                        <tr style="white-space:nowrap">
                                            <td>SR Number</td>
                                            <td>Account</td>
                                            <td>Serial Number</td>
                                            <td>Status</td>
                                            <td>Owner</td>
                                            <td>Priority</td>
                                            <td>Recommendation</td>
                                            <td>Sub Area</td>
                                            <td>Summary</td>
                                            <td>SR Type</td>
                                            <td>Area</td>
                                        </tr>
                                    </thead>
                                    <tbody style="overflow-y:auto; width: 100%;">
                                        @if (Model.ServiceRequests != null)
                                        {
                                            foreach (var sr in Model.ServiceRequests)
                                            {

                                                <tr style="white-space:nowrap">

                                                    <td><a>@sr.SRNumber</a></td>
                                                    <td>not specified</td>
                                                    <td>not specified</td>
                                                    <td>@sr.Status</td>
                                                    <td>Admin</td>
                                                    <td>@sr.Priority</td>
                                                    <td>not specified</td>
                                                    <td>@sr.SubArea</td>
                                                    <td>@sr.Summary</td>
                                                    <td>@sr.Type</td>
                                                    <td>@sr.Area</td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

